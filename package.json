{
  "name": "pv-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "pocketbase": "cd pocketbase && pocketbase serve --http=0.0.0.0:8090",
    "migrate": "cd pocketbase && .\\pocketbase.exe migrate up",
    "build": "next build --turbopack",
    "export": "next export",
    "start": "next start",
